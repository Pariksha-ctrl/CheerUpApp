<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cheerupapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.DanceSongDetailScrollingActivity"
            android:label="@string/title_activity_dance_song_detail_scrolling"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".activities.DanceSongContent" />
        <activity
            android:name=".activities.AddSongForDanceScrollingActivity"
            android:label="@string/title_activity_add_song_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ExercisePageSixthActivity" />
        <activity android:name=".DancePageFourthActivity" />
        <activity android:name=".MeditationPageFourthActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MorningActivitiesPageThirdActivity" />
        <activity android:name=".WelcomePageSecondActivity" />
    </application>

</manifest>